<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view_btn_sec1_id"
        android:layout_width="0dp"
        android:layout_height="241dp"
        app:layout_constraintBottom_toTopOf="@+id/view_btn_sec2_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_titel_id" />

    <Button
        android:id="@+id/gpa_cal_btn"
        android:layout_width="148dp"
        android:layout_height="56dp"
        android:layout_marginEnd="28dp"
        android:fontFamily="@font/allerta"
        android:text="@string/gpa_cal_btn"
        app:backgroundTint="@color/location_btn_id"
        app:circularflow_radiusInDP="50px"
        app:cornerRadius="50px"
        app:layout_constraintBottom_toBottomOf="@+id/view_btn_sec1_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_image_id"
        app:layout_constraintVertical_bias="0.575" />

    <View
        android:id="@+id/view_titel_id"
        android:layout_width="410dp"
        android:layout_height="125dp"
        android:background="#E91E63"
        app:layout_constraintBottom_toTopOf="@+id/view_btn_sec1_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_btn_sec2_id"
        android:layout_width="413dp"
        android:layout_height="237dp"
        app:layout_constraintBottom_toTopOf="@+id/view_nav_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_btn_sec1_id" />

    <View
        android:id="@+id/view_nav_id"
        android:layout_width="0dp"
        android:layout_height="135dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_btn_sec2_id" />

    <TextView
        android:id="@+id/titel_main_id"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:fontFamily="@font/allerta"
        android:gravity="center"
        android:text="@string/titel_main_id"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view_titel_id"
        app:layout_constraintEnd_toEndOf="@+id/view_titel_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.491" />

    <View
        android:id="@+id/view_image_id"
        android:layout_width="0dp"
        android:layout_height="152dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_btn_sec1_id" />

    <ImageView
        android:id="@+id/image_note_id"
        android:layout_width="126dp"
        android:layout_height="99dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_image_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_titel_id"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_baseline_event_note_24" />

    <ImageView
        android:id="@+id/image_cal_id"
        android:layout_width="124dp"
        android:layout_height="95dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_image_id"
        app:layout_constraintEnd_toEndOf="@+id/view_image_id"
        app:layout_constraintHorizontal_bias="0.831"
        app:layout_constraintStart_toEndOf="@+id/image_note_id"
        app:layout_constraintTop_toTopOf="@+id/view_image_id"
        app:layout_constraintVertical_bias="0.438"
        app:srcCompat="@drawable/ic_baseline_calculate_24" />

    <Button
        android:id="@+id/special_notes_btn"
        android:layout_width="139dp"
        android:layout_height="58dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/allerta"
        android:text="@string/special_Notes_btn"
        app:backgroundTint="@color/location_btn_id"
        app:circularflow_radiusInDP="50px"
        app:cornerRadius="50px"
        app:layout_constraintBottom_toBottomOf="@+id/view_btn_sec1_id"
        app:layout_constraintEnd_toStartOf="@+id/gpa_cal_btn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_image_id"
        app:layout_constraintVertical_bias="0.612" />

    <ImageView
        android:id="@+id/image_location_id"
        android:layout_width="0dp"
        android:layout_height="83dp"
        app:layout_constraintBottom_toTopOf="@+id/view_btn_sec2_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_btn_sec1_id"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_location_on_24" />

    <Button
        android:id="@+id/location_btn_id"
        android:layout_width="147dp"
        android:layout_height="55dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/allerta"
        android:text="@string/location_btn_id"
        app:backgroundTint="@color/location_btn_id"
        app:circularflow_radiusInDP="50px"
        app:cornerRadius="50px"
        app:layout_constraintBottom_toBottomOf="@+id/view_btn_sec2_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_location_id"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="58dp"
        android:layout_height="43dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="78dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_nav_id"
        app:srcCompat="@drawable/ic_baseline_home_24" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/view_nav_id"
        app:srcCompat="@drawable/ic_baseline_menu_24" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="49dp"
        android:layout_height="40dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/view_nav_id"
        app:srcCompat="@drawable/ic_baseline_notifications_none_24" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="47dp"
        android:layout_height="39dp"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_nav_id"
        app:srcCompat="@drawable/ic_baseline_person_24" />

</androidx.constraintlayout.widget.ConstraintLayout>